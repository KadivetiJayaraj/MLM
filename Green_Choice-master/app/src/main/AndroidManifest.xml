<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mlmsoftware">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo2"
        android:supportsRtl="true"
        android:theme="@style/Theme.MLMSoftware"
        android:usesCleartextTraffic="true">
        <activity android:name=".SupportActivity" android:theme="@style/AppTheme"/>
        <activity android:name=".ContactUsActivity" android:theme="@style/AppTheme"/>
        <activity android:name=".AboutUsActivity" android:theme="@style/AppTheme"/>
        <activity
            android:name=".AddMemberActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".WalletActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".DownLineActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".ProfileDetailsActivity" />
        <activity
            android:name=".ProfileActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".DashboardActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".SignupActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>